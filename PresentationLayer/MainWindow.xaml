<Window x:Class="PresentationLayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PresentationLayer"
        xmlns:services="clr-namespace:PresentationLayer.Services"
        xmlns:views="clr-namespace:PresentationLayer.Views"

        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="850"
        services:WindowCloser.EnableWindowClosing="True"
        AllowsTransparency="True"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None">
        <!-- Background="Transparent"> -->
        
    
 
    <Grid>
        <views:RegisterUserView />
    </Grid>
</Window>
